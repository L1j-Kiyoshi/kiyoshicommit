<properties>
<comment>AltSettings config</comment>

<!-- 全体チャット可能Lv -->
<entry key="GlobalChatLevel">30</entry>

<!-- ウィスパー可能Lv -->
<entry key="WhisperChatLevel">5</entry>

<!-- AutoLoot ... 0:NonAuto, 1:Auto with Pet&Summon, 2:Auto without Pet&Summon -->
<entry key="AutoLoot">1</entry>

<!-- ルーティングの範囲 -->
<entry key="LootingRange">3</entry>

<!-- NonPvp setting ... True PvP可能 False PvP不可 -->
<entry key="NonPvP">True</entry>

<!-- 自分でタイトルを変更できるか -->
<entry key="ChangeTitleByOneself">False</entry>

<!-- クラン人数の上限 ... 0の場合、Charisma依存 -->
<entry key="MaxClanMember">0</entry>

<!-- クラン連合の有無(君主の他クランへの加入) True to On, False to Off -->
<entry key="ClanAlliance">True</entry>

<!-- PT人数の上限 -->
<entry key="MaxPT">8</entry>

<!-- ChatPT人数の上限 -->
<entry key="MaxChatPT">8</entry>

<!-- 模擬戦の赤ネーム・デスペナルティの有無 True to On, False to Off -->
<entry key="SimWarPenalty">True</entry>

<!-- キャラクター作成時のスタート地点 ... 2005 新隠された渓谷 8013 ドロモンドの部屋 -->
<entry key="StartMapId">2005</entry>

<!-- リスタート時に特定の地点に戻る True to On, False to Off -->
<entry key="GetBack">False</entry>

<!-- ワールドマップ上のアイテムの削除方法 -->
<!-- none - ワールドマップ上のアイテムを削除しない -->
<!-- std - 各アイテムがワールドマップ上に置かれてから指定時間が経過した際に削除 -->
<!-- auto - 指定時間毎に全てのアイテムを一括削除 -->
<entry key="ItemDeletionType">std</entry>

<!-- ワールドマップ上のアイテムを自動的に削除するまでの時間（分）1-35791 -->
<entry key="ItemDeletionTime">10</entry>

<!-- 指定した範囲内にPCがいるとアイテムを削除しない -->
<!-- ItemDeletionTypeがstdの場合、この値は無視される 0-5（0を指定するとPCがいても削除）-->
<entry key="ItemDeletionRange">5</entry>

<!-- GMshop setting... True GMshop有り, False GMshop無し -->
<entry key="GMshop">False</entry>

<!-- GMshopのIDの最小値最大値をspawnlist_npcテーブルのidから指定 -->
<entry key="GMshopMinID">1</entry>
<entry key="GMshopMaxID">34</entry>

<!-- Whoコマンドの使用可否 ... Trueで使用可能 -->
<entry key="WhoCommand">True</entry>

<!-- 復活のポーションの入手 ... Trueで入手あり、falseで入手なし -->
<entry key="RevivalPotion">False</entry>

<!-- 戦争時間(d:日 h:時 m:分) -->
<entry key="WarTime">2h</entry>

<!-- 戦争間隔(d:日 h:時 m:分) -->
<entry key="WarInterval">4d</entry>

<!-- 天候システム ... Trueで天候を定期的に変更、Falseで晴れ固定 -->
<entry key="WeatherSystem">False</entry>

<!-- 天候を変更する間隔（単位：秒、デフォルトは1時間毎）-->
<entry key="WeatherInterval">3600</entry>

<!-- 範囲出現(spawnlistでlocx1,locy1,locx2,locy2が設定されている)モンスターのホームポイント設定 -->
<!-- True:ホームポイントを持たせる, False:ホームポイントを持たせない -->
<entry key="SpawnHomePoint">True</entry>

<!-- ホームポイントからのランダム出現距離 -->
<entry key="SpawnHomePointRange">8</entry>

<!-- SpawnHomePointCount,SpawnHomePointDelayはホームポイントの適用を回避する為の設定です。-->
<!-- (ボスに適用しないようにするなど) -->
<!-- ホームポイント設定を適用する最小count -->
<!-- (2の場合、countが2以上のspawnlistのみホームポイントを設定する) -->
<entry key="SpawnHomePointCount">2</entry>
<!-- ホームポイント設定を適用する最大min_respawn_delay -->
<!-- (100の場合、min_respawn_delayが100以下のspawnlistのみホームポイントを設定する) -->
<entry key="SpawnHomePointDelay">100</entry>

<!-- サーバ起動時にボス時間だった場合、出現させるか否か。Falseの場合は次の周期から開始する。-->
<entry key="InitBossSpawn">True</entry>

<!-- 精霊の石の設置数 -->
<entry key="ElementalStoneAmount">100</entry>

<!-- アジトの税金の支払い期限(日) -->
<entry key="HouseTaxInterval">10</entry>

<!-- マジックドールの最大数 -->
<entry key="MaxDollCount">1</entry>

<!-- リターントゥネイチャーの使用 ... True to On, False to Off -->
<entry key="ReturnToNature">False</entry>

<!--- NPC(Summon, Pet)が所持可能なアイテム数 -->
<entry key="MaxNpcItem">8</entry>

<!-- 個人倉庫に格納可能なアイテム数 -->
<entry key="MaxPersonalWarehouseItem">100</entry>

<!-- 追加倉庫に格納可能なアイテム数 -->
<entry key="MaxAdditionalWarehouseItem">20</entry>

<!-- 血盟倉庫に格納可能なアイテム数 -->
<entry key="MaxClanWarehouseItem">200</entry>

<!-- 血盟倉庫使用履歴の保存日数 ... デフォルト3日、0を指定した場合削除しない(非推奨) -->
<entry key="MaxClanWarehouseHistoryDays">3</entry>

<!-- Lv30以上のキャラクターを削除する際、7日間の猶予を設けるか True to On, False to Off -->
<entry key="DeleteCharacterAfter7Days">False</entry>

<!-- NPCが死亡してから消えるまでの時間（秒）-->
<entry key="NpcDeletionTime">10</entry>

<!-- DefaultCharacterSlot... デフォルトのキャラクタースロット数 -->
<entry key="DefaultCharacterSlot">4</entry>

<!-- PKLOGの使用 True to On, False to Off -->
<entry key="PkLog">True</entry>

<!-- BOSS_SPAWN_LOGの使用 True to On, False to Off　初期配置には非適用 -->
<entry key="BossSpawnLog">False</entry>

<!-- BOSS_END_LOGの使用 True to On, False to Off -->
<entry key="BossEndLog">False</entry>

<!-- 地獄使用 True to On, False to Off -->
<entry key="Hell">False</entry>

<!-- PKリミット数  *地獄使用無しの場合無効 *ブルーメッセージはlimit-5から表示 -->
<entry key="PkCountLimit">8</entry>

<!-- ペット名の変更 -->
<entry key="RenamePetName">false</entry>

<!-- ペットレース参加最小人数　*1～9で設定 -->
<entry key="PetRaceMinPlayer">1</entry>

<!-- ペットレース周回数　*1～4で設定 -->
<entry key="PetRaceMaxLap">3</entry>

<!-- アルティメットバトル開催 True to On, False to Off -->
<entry key="UltimateBattle">True</entry>

<!-- アルティメットバトル予告(分) 11なら開始11分前にNPCが予告する -->
<entry key="UltimateBattleNotice">11</entry>

<!-- デスマッチ参加最小人数 *2未満は本鯖基準 -->
<entry key="DeathMatchMinPlayer">2</entry>

<!-- +6以上の装飾品強化時の追加ボーナスを、+7以降の強化にも適用するか -->
<!-- std - 本家同様、ボーナスは+6になった1回のみとする -->
<!-- ext - +7以降も強化毎にボーナス値を上昇させる -->
<entry key="AccessoryEnchantBonus">std</entry>

<!-- リサイクルシステム -->
<!-- True ... ゴミ箱にアイテムを捨てた時、買取価格が設定されているアイテムはアデナに換金 -->
<!-- False ... 従来どおりアイテムを削除 -->
<entry key="RecycleSystem">False</entry>

</properties>
